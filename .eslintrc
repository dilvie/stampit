// Add our overrides as necessary
{
  "extends": [
    "eslint:recommended",
    "plugin:import/recommended",
    "plugin:import/warnings"
  ],
  "rules": {
    "object-curly-spacing": [2, "never"],
    "no-param-reassign": 0,
    "comma-dangle": 0,
    "no-confusing-arrow": 0,
    "prefer-rest-params": 0,
    "no-use-before-define": [2, "nofunc"],
  }
}
